{
  "name": "auto-push-email",
  "version": "1.0.0",
  "packageManager": "pnpm@7.12.1",
  "description": "Automatically send daily emails to people you care about",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch core --ext ts --exec pnpm push",
    "push": "esno ./core/index.ts"
  },
  "keywords": [
    "GitHub Actions",
    "Auto Push"
  ],
  "author": "flingyp",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.6",
    "@types/request-promise": "^4.1.48",
    "esno": "^0.16.3",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@flypeng/tool": "^1.4.2",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.7.8",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  }
}
